#1
import math                             # Импортируем библиотеку, которая нам даст математические функции всякие

def f(a, b, h):       # Создаём функцию, которая принимает на вход 3 аргумента, начальное значение, конечное значение и шаг
    y = []                                                       # Создаём список (массив)
    while True:                                                  # Создаём цикл. Пока верно цикл продолжается. Мы составляем типо таблицу, поэтому просто от одного предела до другого идём, с определённым шагом
        w = 1 / ((2 - a ** 2) * math.sqrt(2 - (a ** 2)))         # Функция которая у тебя написана в задание. Значение этой функции записывается в эту переменную
        y.append(w)                                              # Добавляем в список
        a += h                                                   # Добавляем шаг (выходим на новое значение функции w )
        if a > b:                                                # Проверяем не выходит ли наша нынешная а за предел значения которого мы ей дали. то есть b.
            break                                                # Если да, то цикл завершается. Если нет то ебашим пока не завершиться
    print(y)                                                     # В конце распечатываем масив

# Это функция. Теперь её надо вызвать, но сначала мы должны дать программе собстенно 3 аргумента, начальное значение, конечное значение и шаг

a = float(input('Введите начало множества: '))
b = float(input('Введите конец множества: '))
h = float(input('Введите шаг: '))

f(a, b, h)  # Вызываем функцию и всё)


#2

def fun_str(string):                                                                    # Создаём функцию. Она принимает строку которую мы вводим
    my_list = []                                                                        # Создаём масив куда будем добавлять всю инфу
    my_list.append('Количество слов ' + str(len(string.split())))                       # Добавляем в наш масив количество слов
    my_list.append('Длина строки ' + str(len(string)))                                  # Добавляем в наш масив количество букв в строчке
    count_b = 0                                               # создаём переменные типо счётчика. Счётчик считай каждый отдельный случай чтобы потом вывести его
    count_n = 0
    count_s = 0
    for i in string:                                          # Цикл. Для каждого элемента строки( для каждого символа) проверяем
        i = str(i)                                            # Переводим символ в строку, чтобы это ебанный питон ошибку типа данных не выводил ))
        if i == '!' or i == '?' or i == '#' or i == '/' or i == '*' or i == '&':          # Проверка на спец символы
            count_s += 1                                                                    # И если они есть, то счётчик прибавит
        elif i.isalpha() == True:                                                         # Проверка на буквы. Если это буква, то счётчик запишет в буквы
            count_b += 1
        elif i.isdigit() == True:                                                         # Проверка на цифры. Если это цифра, то счётчик запишет в цифры
            count_n += 1

    my_list.append('Количество букв в строке ' + str(count_b))        # Теперь мы сразу добавляем в масив все наши значения. Сколько букв цифр и спец символов
    my_list.append('Количество цифр в строке ' + str(count_n))
    my_list.append('Количество спецсимволов в строке ' + str(count_s))
    print(my_list)                # Ну и выводим нашу красоту


string = input("Введите строку ")  # Сюда подаётся наша строка на вход
fun_str(string)     # Вызываем функцию и кайфуем

# Если он будет спрашивать что это такое (isdigit(),isalpha(),append() ) То не растеряйся и ответь ему.

# isdigit() метод любой строки которая определяет это строка из цифр или нет
# isalpha() это метод соответственно который определяет буквы это или нет
# append() а этот метод добавляет в массив ( который мы пустой создали my_list = [] ) это всё добро



#3

my_list = ['Фонк', 'Дота', 'Деньги', 'Сессия', 'Девушки', 'Машины', 'Москва', 'Донбасс', 'Ыыыы'] # просто создаём список и рандомные слова туда. Можешь изменить, я для примера взял
my_file = open("SexFile.txt", "w+")           # Создаём файл. w+ эта хуета нам нужна для того, чтобы
                                                # 300 тысяч файлов при каждом запуске не создавать, он каждый раз перезаписывает. Название файла тоже можешь поменять
for i in range(len(my_list)):       # Создаём цикл. Для каждого из элементов этого списка (масива который мы создали)
    my_file.write(my_list[i] + "\n")    # В файл записываем наше одно слово из массива и переходим на следующую строку файла.
my_file.close()                         # закрываем файл с которым работали (их типо надо закрывать чтобы всё ок было)


def find_str(number):              # создаём функцию аргумент для нее наша введённая строка
    count = 0                    # создаём счетчик
    with open("DFile.txt", "r") as file:  # открываем файл для чтения(типо мы должны указать для чего, чтобы он понимал что мы делать будем с файлом)
        for line in file:               # цикл. Для каждой строки( то есть слова)  из файла
            count += 1                     # Счётчик увеличиваем, чтобы считать на какой мы строке
            if number == count:                # Если нужная нам строка равно нынешней ( проверка благодяря счётчику)
                print(line)                     # мы печатаем нашу строку


number = int(input()) # вводим наше число

if number > len(my_list): # если число введённое больше чем слов в нашем массиве, выводим предупреждение чтобы слово поменьше ввели 
    number = number = int(input('Введите число не превышающее ' + str(len(my_list)) + ' - ')) # вводим теперь нормальное число
find_str(number) # вызываем функцию и кайфуем
